    IEnumerable<CompanyIdClaimValue> companyClaimValues = ClaimHelper.GetClaimsByType<CompanyIdClaimValue>(SmdClaimTypes.CompanyId);
    string CompanyName = companyClaimValues != null && companyClaimValues.Any() ? companyClaimValues.ElementAt(0).CompanyName : "Company Not Loaded";
    string CompanyCity = companyClaimValues != null && companyClaimValues.Any() ? companyClaimValues.ElementAt(0).CompanyCity : "Company Not Loaded";
    string CompanyAddressLine1 = companyClaimValues != null && companyClaimValues.Any() ? companyClaimValues.ElementAt(0).addressLine1 : "Company Not Loaded";
    string CompanyLogo = companyClaimValues != null && companyClaimValues.Any() ? companyClaimValues.ElementAt(0).CompanyLogo : "Claim not found";
    string userName = companyClaimValues != null && companyClaimValues.Any() ? companyClaimValues.ElementAt(0).UserName : "Claim not found";
    string MobileNo = companyClaimValues != null && companyClaimValues.Any() ? companyClaimValues.ElementAt(0).MobileNumber : "Claim not found";
    string CompanyID = companyClaimValues != null && companyClaimValues.Any() ? companyClaimValues.ElementAt(0).CompanyId.ToString() : "Claim not found";
    string Email = companyClaimValues != null && companyClaimValues.Any() ? companyClaimValues.ElementAt(0).Email : "Claim not found";
    IEnumerable<SmdRoleClaimValue> roleClaim = ClaimHelper.GetClaimsByType<SmdRoleClaimValue>(SmdClaimTypes.Role);
    string RoleName = roleClaim != null && roleClaim.Any() ? roleClaim.ElementAt(0).Role : "Role Not Loaded";
    string RoleId = roleClaim != null && roleClaim.Any() ? roleClaim.ElementAt(0).RoleId : "Role Not Loaded";

    var Name = string.Empty;

    if (!String.IsNullOrEmpty(userName))
    {
       
        Name = userName;
    }
    else
    {
        if (User != null && User.Identity != null)
        {
          
            Name = User.Identity.Name;
        }
    }
}
