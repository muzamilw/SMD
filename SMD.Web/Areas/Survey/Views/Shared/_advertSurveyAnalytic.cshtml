<div data-bind="visible: $root.isAdvertdashboardPollVisible()" style="display: none" class=" col-lg-9 form-horizontal" style="padding-left:0px;">
    <div class="main-box main-box-subpage">
        <div class="row">
            <div class="row">

                <br />
                <div class="col-lg-6 adsubHeading Theme_standard_clr borderbtmline" style="padding-bottom:10px;">
                    Timeline
                </div>
                <div class="col-lg-3 adsubHeading Theme_standard_clr borderbtmline" style="padding-bottom:10px;">
                    <label class="control-label text-right tooltip-float floatNone defaultThemeTxtClr defaultImgUploadImageBtn">Date range </label>
                </div>
                <div class="col-lg-3 adsubHeading Theme_standard_clr borderbtmline" style="padding-bottom:2px;">
                    <select class="form-control" id="ddDateRangeDropDown" data-bind="options: $root.DateRangeDropDown, optionsText: 'name', optionsValue: 'id', value:$root.selecteddateRangeAnalytics, event: { change: $root.getSurvayAnalytics }"></select>
                </div>



            </div>
            <div>
                <div class="col-lg-3"></div>

                <div class="col-lg-2">
                    <label style="padding-top:18px;" class="control-label text-right tooltip-float floatNone defaultThemeTxtClr defaultImgUploadImageBtn">Granularity level </label>
                </div>
                <div class="col-lg-3">
                    <select class="form-control margin10 pull-left" id="ddGranularityDropDown" data-bind="options: $root.granularityDropDown, optionsText: 'name', optionsValue: 'id', value:$root.selectedGranularityAnalytics, event: { change: $root.getSurvayAnalytics }"></select>
                </div>

                <div class="col-lg-1">
                    <label style="padding-top:18px;" class="control-label text-right tooltip-float floatNone defaultThemeTxtClr defaultImgUploadImageBtn">Status </label>
                </div>
                <div class="col-lg-3">
                    <select class="form-control margin10 pull-left" id="ddCampaignStatusDropDown" data-bind="options: $root.CampaignStatusDropDown, optionsText: 'name', optionsValue: 'id', value:$root.selectedCampStatusAnalytics, event: { change: $root.getSurvayAnalytics }"></select>
                </div>

            </div>

            
        </div>
        <div class="row main-box">
            <div class="col-lg-12">
                <div class=" margin10">
                    <!-- ko if: $root.selectedCampStatusAnalytics() === 1  -->
                    <div style="height:200px;" data-bind="morris: {type: 'Line', data: $root.SQAnalyticsData, xkey: 'Granual', ykeys: ['openStats', 'Stats'], labels: ['Opened', 'Answered'], parseTime:false, setAxisAlignFirstX: true, lineColors: ['blue','green'] }"></div>
                    <!-- /ko -->
                    <!-- ko if: $root.selectedCampStatusAnalytics() === 2  -->
                    <div style="height:200px;" data-bind="morris: {type: 'Line', data: $root.SQAnalyticsData, xkey: 'Granual', ykeys: ['openStats', 'Stats'], labels: ['Opened', 'Skipped'], parseTime:false, setAxisAlignFirstX: true, lineColors: ['blue','red'] }"></div>
                    <!-- /ko -->


                </div>
            </div>

        </div>
        <div class="row">
            <div class="adsubHeading Theme_standard_clr borderbtmline">
                <br />
                Card 1 & 2 results
            </div>
        </div>
        <div class="row main-box">
            <div class="col-lg-6">
                <!--ko if: $root.selecteddateRangeAnalytics() == 1 -->
                <div>Date range : Last 30 days </div>
                <!--/ko-->
                <!--ko if: $root.selecteddateRangeAnalytics() == 2 -->
                <div>Date range : All time </div>
                <!--/ko-->

                <div data-bind="morris: {type: 'Donut', data: $root.CampaignRatioAnalyticData, resize:true, colors: ['green', 'blue' ]  }" style="width:250px;height:250px;padding-left:50px;"></div>
                
            </div>
            <div class="col-lg-6">
                <table style="margin-top:70px;">
                    <thead>
                        <tr>
                            <th>
                                &nbsp;
                            </th>
                            <th class="padingL15 text-right">
                                All time
                            </th>
                            <th class="padingL15 text-right">
                                30 days
                            </th>
                        </tr>
                    </thead>
                    <!-- ko template: { name: 'CampaignTblAnalyticsDataTemplate' , foreach: $root.CampaignTblAnalyticsData } -->
                    <!-- /ko -->
                </table>
            </div>

        </div>
        <div class="row">
            <div class="adsubHeading Theme_standard_clr borderbtmline">
                <br />
                Campaign ROI
            </div>
        </div>
        <div class="row">


            <div class="col-lg-12">
                <table class="roitbl" style="">
                    <!-- ko template: { name: 'ROITblAnalyticsDataTemplate' , foreach: $root.CampaignROItblAnalyticData } -->
                    <!-- /ko -->
                </table>
            </div>

        </div>
        <div class="clearfix">
        </div>
        <div class="clearfix">
        </div>
    </div>

</div>

<script id="CampaignTblAnalyticsDataTemplate" type="text/html">
    <tr>
        <td><span data-bind="text: label"></span></td>
        
        <td class="padingL15 text-right">
            <!-- ko if: ordr === 4  -->
            $
            <!-- /ko -->
            <span data-bind="text: All_time"></span>
        </td>
        <td class="padingL15 text-right">
            <!-- ko if: ordr === 4  -->
            $
            <!-- /ko -->
            <span data-bind="text: C30_days"></span>
        </td>
    </tr>
</script>


<script id="ROITblAnalyticsDataTemplate" type="text/html">
    <tr>
        <td><span data-bind="text: label"></span></td>

        <td class="text-right">
            <span data-bind="text: value"></span>%
        </td>
       
    </tr>
</script>