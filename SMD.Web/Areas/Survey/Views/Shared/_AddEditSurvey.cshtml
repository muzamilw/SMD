        function showVal(val) {
            $("#show_audience").text(GetAudienceCount(val)).change();
            $("#txtPerClick").val(val).change();
        }
        function DefaultStatus() {
            var val = $("#Ranges").val();
            $("#show_audience").text(GetAudienceCount(val)).change();
            $("#txtPerClick").val(val).change();
        }

        function GetAudienceCount(val) {
            while (/(\d+)(\d{3})/.test(val.toString())) {
                val = val.toString().replace(/(\d+)(\d{3})/, '$1' + ',' + '$2');
            }
            return val;
        }

        function PageScroll() {
            $('html, body').animate({ scrollTop: 0 }, 800);

        }
        var sImageLeft = "";
        var sImageRight = "";
        var mode = 1;
        function changeIframeSrc(mode1) {

            mode = mode1;
            $("#iframe").attr("src", "/DamImages?mode=4");
        }
        function iframeLoaded() {
            var iframeBody = $('body', $('#iframe')[0].contentWindow.document);
            $(iframeBody).on('click', '.damImgContainer img', function (event) {
                console.log(mode);
                if (mode == 1) {
                    $("#leftImgPath").attr("src", this.src).change();
                    sImageLeft = this.src;
                    $("#btnDamClose").click();
                } else if (mode == 2) {
                    $("#rightImgPath").attr("src", this.src).change();
                    sImageRight = this.src;
                    $("#btnDamClose").click();
                }
            });
        }

    </script>
