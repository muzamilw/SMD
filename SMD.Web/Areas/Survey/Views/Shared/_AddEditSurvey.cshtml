                                                            <option value="1">Include</option>
                                                            <option value="0">Exclude</option>
                                                        </select>
                                                    </div>
                                                    <div class="sep-line">

                                                    </div>
                                                    <div class="count_token_opt_cnt">
                                                        <input type="text" data-autocomplete="off" class="" id="searchLanguages" placeholder="Enter a language" />
                                                    </div>

                                                    <div class="clearfix">

                                                    </div>
                                                </div>

                                            </div>

                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div class="col-lg-3 text-right">
                                            <div class="bs-example-tooltips tooltip-float-width">
                                                <a class="input-group-addon" tabindex="-1" href="#" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="" data-original-title="" title="">
                                                    <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                                </a>
                                            </div>
                                            <label for="txtLanguages" class="control-label text-right tooltip-float">Professions</label>
                                        </div>
                                        <div class="col-lg-6">
                                            <div>

                                                <div id="lang_token_cnt">
                                                    <!-- ko template: { name: 'SurveyQuestionIndustryTemplate' , foreach: SurveyQuestionTargetCriteria } -->
                                                    <!-- /ko -->
                                                </div>
                                                <div class="clearfix"></div>
                                                <div id="land_edt">
                                                    <div class="coun_in_ex_cnt">
                                                        <select id="in_ex_count_opt" class="form-control" data-bind="value: $root.selectedIndustryIncludeExclude">
                                                            <option value="1">Include</option>
                                                            <option value="0">Exclude</option>
                                                        </select>
                                                    </div>
                                                    <div class="sep-line">

                                                    </div>
                                                    <div class="count_token_opt_cnt">
                                                        <input type="text" data-autocomplete="off" class="" id="searchIndustries" placeholder="Enter an industry" />
                                                    </div>

                                                    <div class="clearfix">

                                                    </div>
                                                </div>

                                            </div>

                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <div class="col-lg-3 text-right">
                                            <div class="bs-example-tooltips tooltip-float-width">
                                                <a class="input-group-addon" tabindex="-1" href="#" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-content="" data-original-title="" title="">
                                                    <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                                </a>
                                            </div>
