@{
    ViewBag.Title = "Store Details";
}

<div data-bind="visible: $root.isEditorVisible()" style="display: none;">
    <div class="col-lg-12">
        <div class="row">
            <div class="col-lg-12">
                <ol class="breadcrumb">
                    <li><a href="#" class="my-navigator">Home</a></li>
                    <li><a href="#" class="my-navigator">Survey Questions</a></li>
                </ol>
                <div class="tab-content dynamicNameHeading" style="height:41px;">
                    <h1 data-bind="text:$root.titleText">Add new survey</h1>
                </div>
            </div>
        </div>
        <div class="row" id="user-profile">
            <div class="col-lg-12 col-md-8 col-sm-8" style="padding-bottom: 10px;">
                <div class="clearfix ">
                    <div class="col-xs-12" style="text-align: right; padding-right:0;margin-bottom:10px;">
                        <button class="btn  btn-success" id="saveBtn" data-bind="click:$root.onSubmitSurveyQuestion, visible: $root.canSubmitForApproval">Submit for Approvals</button>
                        <button class="btn  btn-info" id="saveBtn" data-bind="click:$root.onSaveSurveyQuestion, enable: $root.hasChangesOnQuestion, css: { 'btn-warning': $root.hasChangesOnQuestion }">Save</button>
                        <button class="btn btn-default" id="closeBtn" data-bind="click: $root.closeEditDialog">Close</button>
                    </div>
                </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12" data-bind="with: $root.selectedQuestion">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="main-box no-header clearfix">
                            <div class="col-lg-12 form-horizontal" style="    padding: 20px;">
                                <div class="tabsdescriptions" style="margin-left:3px;">
                                    <div class="col-lg-9" style="padding-left: 0 !important;">
                                        <p class="group-header-description" style="margin-top:-1px;">
                                            Manage the basic details of your survey in this section.
                                        </p>
                                    </div>
                                    <div class="col-lg-3 pull-left" style="margin-top:-9px;padding-right:0px;">
                                        <div class="col-lg-5 text-right">
                                            <div class="bs-example-tooltips tooltip-float-width">
                                                <a class="input-group-addon" tabindex="-1" href="#" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="right" data-original-title="" title="">
                                                    <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                                </a>
                                            </div>
                                            <label class="control-label">Status</label>
                                        </div>
                                        <div class="col-lg-6">

                                            <label class="control-label bold" data-bind="text:StatusValue()">Archived</label>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-lg-9">
                                    <div class="form-group" style="margin-bottom:14px;">
                                        <div class="col-lg-3 text-right">

                                            <label for="txtSurveyTtile" class="control-label text-right tooltip-float">Survey Title</label>
                                        </div>
                                        <div class="col-lg-6">
                                            <input type="text" name="Campaign Name" class="form-control" id="txtSurveyTtile" placeholder="Enter Survey title" data-bind="value: Question">

                                        </div>
                                    </div>
                                    <div class="form-group" style="margin-bottom:14px;">
                                        <div class="col-lg-3 text-right" style="padding-top:12px;">

                                            <label for="txtDisplayQuestion" class="control-label text-right tooltip-float">Survey Display Question</label>
                                        </div>
                                        <div class="col-lg-6">
                                            <input type="text" style="height:60px;" name="" class="form-control height60 font24px" id="txtDisplayQuestion" placeholder="Survey Display Question" data-bind="value: DisplayQuestion">

                                        </div>
                                    </div>
                                    <div class="form-group" style="margin-bottom:25px;">
                                        <div class="col-lg-3 text-right" style="padding-top:12px;">

                                            <label for="txtSurveyDesc" class="control-label text-right tooltip-float">Survey Description</label>
                                        </div>
                                        <div class="col-lg-6">
                                            <textarea class="form-control" id="txtSurveyDesc" rows="5" placeholder="Enter Description" data-bind="value: Description"></textarea>

                                        </div>
                                    </div>

                                    <div class="form-group" style="margin-bottom:25px;">
                                        <div class="col-lg-1"></div>
                                        <div class="col-lg-5">
                                            <div class="col-lg-4 text-right" style="padding-top:12px;">

                                                <label for="txtSurveyDesc" class="control-label text-right tooltip-float">Left Answer</label>
                                            </div>
                                            <div class="col-lg-6">

                                                <div class="col-lg-12">

                                                    <div style="width:210px;text-align:center;">
                                                        <img style="width:auto;height:80px;max-width:200px;" data-bind="visible:LeftPictureBytes()==null  ,attr: { src:  LeftPicturePath() }" src="/Content/Images/Company_Default.png" />
                                                        <img style="width:auto;height:80px;max-width:200px;" data-bind="visible:LeftPictureBytes()!=null,attr: { src: LeftPictureBytes }" />

                                                    </div>

                                                </div>

                                                <div class="col-lg-12" style="margin-top:15px;text-align:center;width:240px;padding-right:0px;">
