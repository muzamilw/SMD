<script>
    var map;
    var center;
    var circle = null;
    function initializeMap(lat, long, place) {
        // LOCATION IN LATITUDE AND LONGITUDE.
        center = new google.maps.LatLng(long, lat);
        // MAP ATTRIBUTES.
        var mapAttr = {
            center: center,
            zoom: 15,
            mapTypeId: google.maps.MapTypeId.TERRAIN
        };
        var myLatLng = { lat: parseFloat(lat), lng: parseFloat(long) };
        // THE MAP TO DISPLAY.

        map = new google.maps.Map(document.getElementById("mapContainer"), mapAttr);
        google.maps.event.addDomListener(window, 'load', initialize);

        var marker = new google.maps.Marker({
            position: center,
            map: map,
            title: place
        });

    }
    function addRadius(radius) {
        if (circle != null)
            circle.setMap(null);
        circle = new google.maps.Circle({
            center: center,
            map: map,
            radius: parseInt(radius),          // IN METERS.
            fillColor: '#FF6600',
            fillOpacity: 0.3,
            strokeColor: "#FFF",
            strokeWeight: 0         // DON'T SHOW CIRCLE BORDER.
        });

    }


</script>
