                                                        <div class="bs-example-tooltips tooltip-float-width">
                                                            <a class="input-group-addon" tabindex="-1" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="left" data-html="true" data-content="" data-original-title="" title="">
                                                                <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                                            </a>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6" style="text-align:left;">
                                                        <div class="fileUpload btn btn-primary">
                                                            <span>Upload </span>
                                                            <input type="file" class="upload" id="image_files" data-bind="files: { onLoaded: $root.storeLeftImageCallback, fileFilter: 'image.*'}" />
                                                        </div>
                                                    </div>

                                                </div>


                                            </div>
                                        </div>
                                        <div class="col-lg-6">
                                            <div class="col-lg-3 text-right" style="padding-top:12px;">

                                                <label for="txtSurveyDesc" class="control-label text-right tooltip-float">Right Answer</label>
                                            </div>
                                            <div class="col-lg-6">

                                                <div class="col-lg-12">

                                                    <div style="width:210px;text-align:center;">
                                                        <img style="width:auto;height:80px;max-width:200px;" data-bind="visible:RightPictureBytes()==null,attr: { src:  RightPicturePath() }" src="/Content/Images/Company_Default.png" />
                                                        <img style="width:auto;height:80px;max-width:200px;" data-bind="visible:RightPictureBytes()!=null,attr: { src: RightPictureBytes }" />
                                                    </div>

                                                </div>

                                                <div class="col-lg-12" style="margin-top:15px;text-align:center;width:240px;padding-right:0px;">
                                                    <div class="col-lg-5" style="padding-right:10px;">
                                                        <div class="bs-example-tooltips tooltip-float-width">
                                                            <a class="input-group-addon" tabindex="-1" href="#" data-bind="click: function(){}" data-trigger="focus" style="text-decoration: none; border: 0; cursor: pointer;" data-container="body" data-toggle="popover" data-placement="left" data-html="true" data-content="" data-original-title="" title="">
                                                                <span class="label label-circle-question">&nbsp;&nbsp;?&nbsp;&nbsp;</span>
                                                            </a>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6" style="text-align:left;">
                                                        <div class="fileUpload btn btn-primary">
