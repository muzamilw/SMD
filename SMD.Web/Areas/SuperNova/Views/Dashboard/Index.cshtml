@{

}
<div class="row">
    <div class="row" id="analyticsdashboardBindingRoot" style="z-index:0;">
        <div class="row">
            <div class="col-lg-12">
                <br />
                <h1 style="padding-left:7px!important;">DashBoard </h1>
            </div>
        </div>
        <div class="col-lg-12">

            <div class="main-box">
                <div class="row">
                    <div class="col-lg-8"></div>
                    <div class="col-lg-4">
                        <label class="control-label text-right pull-left margin10" style="padding-top:8px;">Date Range </label>
                        
                        <input type="text" class="form-control margin10 pull-left" style="width:35%;" id="FromDate" data-bind="datepicker:analyticFromdate">
                        <input type="text" class="form-control margin10" style="width:35%;" id="ToDate" data-bind="datepicker:analyticTodate">
                   

                    </div>
                </div>
                <div class="row">

                    <div class="col-lg-6">
                        <h2 class="graphheaderleft">Top 10 Spenders on platform</h2>
                        <div class="form-group lightgray-bg graph-box-analytics">
                            
                        </div>
                        <h2 class="graphheaderleft">Revenue over time</h2>
                        <div class="form-group lightgray-bg graph-box-analytics" style="margin-right:10px">
                        </div>
                        <div class=" margin10">

                            <select class="form-control" style="width:35%!important;" id="ddpCorrectAns" data-bind="options: $root.granularityDropDown, optionsText: 'name', optionsValue: 'Id'"></select>

                        </div>
                    </div>
                    <div class="col-lg-6">
                        <h2 class="graphheaderright">Top 10 campaigns by spend</h2>
                        <div class="form-group lightgray-bg graph-box-analytics" style="margin-right:10px">
                           

                        </div>
                        @*<h2 class="graphheaderright">Afilliate Vs SMD Revenue</h2>
                        <div class="form-group" style="margin-right:10px">
                            <div class="graph-box lightgray-bg">
                               
                            </div>

                        </div>*@
                        <h2 class="graphheaderright">Active Users</h2>
                        <div class="form-group lightgray-bg graph-box-analytics" style="margin-right:10px">


                            <div class="margin-Top-30" data-bind="morris: {type: 'Bar', data: $root.activeUser, xkey: 'y', ykeys: ['a'], labels: ['Active Users']}" style="width: 600px; float: left"></div>

                        </div>
							
						
                        </div>
                    
                    </div>
                </div>
            </div>

        </div>

    </div>
</div>
@section Scripts{
<script type="text/javascript">
    require(["analytic/analytic.view"]);
	

    
</script>
}