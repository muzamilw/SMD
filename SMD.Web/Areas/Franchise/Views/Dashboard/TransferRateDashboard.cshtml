@section Scripts{
    <script type="text/javascript">
        //require(["FranchiseDashboard/profileQuestionApp.view", "common/confirmation.view"]);
        $("#btnTransferRates").css("background-color", "#34b9c7");
    </script>
    <script>
        function getParameter(url, param) {
            if (url.indexOf('?') == -1)
                return 1;
            url = url.split('?');
            var pattern = new RegExp(param + '=(.*?)(;|&|$)', 'gi');
            return url[1].split(pattern)[1];
        }
    </script>
