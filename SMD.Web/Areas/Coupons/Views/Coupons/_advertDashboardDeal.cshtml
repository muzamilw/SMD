                     <div class="col-lg-6"></div>
                     <div class="col-lg-6">
                         <select class="form-control margin10 pull-left" style="width:43%!important;" id="ddGranularityDropDown" data-bind="options: $root.granularityDropDown, optionsText: 'name', optionsValue: 'id', value:$root.selectedGranularityAnalytics, event: { change: $root.getDealsAnalytics }"></select>
                         <select class="form-control margin10 pull-left" style="width:43%!important;" id="ddDateRangeDropDown" data-bind="options: $root.DateRangeDropDown, optionsText: 'name', optionsValue: 'id', value:$root.selecteddateRangeAnalytics, event: { change: $root.getDealsAnalytics }"></select>

