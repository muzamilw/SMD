                 <div class="row main-box">
                     <div class="col-lg-12">
                         <div class=" margin10">
                             <div class="margin-Top-30" data-bind="morris: {type: 'Line', data: $root.DealsAnalyticsData, xkey: 'Granual', ykeys: ['openStats', 'ConvrsionStats'], labels: ['Total Impressions', 'Referred'], parseTime:false, setAxisAlignFirstX: true, lineColors: ['blue','red'] }"></div>
                            
                         </div>
                     </div>

