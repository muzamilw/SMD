         <div data-bind="visible: $root.isAdvertdashboardSurvayVisible()" style="display: none" class=" col-lg-9 form-horizontal" style="padding-left:0px;">
    <div class="main-box main-box-subpage">
        <div class="row">
            <div class="col-lg-12">
                <br />
                <h1 style="padding-left:7px!important;">Ad campaign timeline</h1>
            </div>
        </div>
        <div>
            <div class="col-lg-4"></div>
            <div class="col-lg-8">
                <select class="form-control margin10 pull-left" style="width:28%!important;" id="ddGranularityDropDown" data-bind="options: $root.granularityDropDown, optionsText: 'name', optionsValue: 'id', value:$root.selectedGranularityAnalytics, event: { change: $root.getSurvayAnalytics }"></select>
                <select class="form-control margin10 pull-left" style="width:28%!important;" id="ddDateRangeDropDown" data-bind="options: $root.DateRangeDropDown, optionsText: 'name', optionsValue: 'id', value:$root.selecteddateRangeAnalytics, event: { change: $root.getSurvayAnalytics }"></select>
                @*<select class="form-control margin10 pull-left" style="width:28%!important;" id="ddCampaignStatusDropDown" data-bind="options: $root.CampaignStatusDropDown, optionsText: 'name', optionsValue: 'id', value:$root.selectedCampStatusAnalytics, event: { change: $root.getSurvayAnalytics }"></select>*@
            </div>

        </div>
        <div class="row main-box">
            <div class="col-lg-12">
                <div class=" margin10">
                    <!-- ko if: $root.selectedCampStatusAnalytics() === 1  -->
                    <div class="margin-Top-30" data-bind="morris: {type: 'Line', data: $root.PQAnalyticsData, xkey: 'Granual', ykeys: ['openStats', 'Stats'], labels: ['Total Impressions', 'Answered'], parseTime:false, setAxisAlignFirstX: true, lineColors: ['blue','red','green'] }"></div>
                    <!-- /ko -->
                    <!-- ko if: $root.selectedCampStatusAnalytics() === 2  -->
                    <div class="margin-Top-30" data-bind="morris: {type: 'Line', data: $root.PQAnalyticsData, xkey: 'Granual', ykeys: ['openStats', 'Stats'], labels: ['Total Impressions', 'Skipped'], parseTime:false, setAxisAlignFirstX: true, lineColors: ['blue','red','green'] }"></div>
                    <!-- /ko -->
                  

                </div>
            </div>

        </div>
        <div class="row main-box margin-Top-30">

            <div class="col-lg-7">
                <div data-bind="morris: {type: 'Donut', data: $root.CampaignRatioAnalyticData, resize:true }" style="width:400px;height:350px;"></div>

            </div>
            <div class="col-lg-5">
                <table style="margin-top:70px;">
                    <thead>
                        <tr>
                            <th>
                                &nbsp;
                            </th>
                            <th class="padingL15 text-right">
                                All time
                            </th>
                            <th class="padingL15 text-right">
                                30 days
                            </th>
                        </tr>
                    </thead>
                    <!-- ko template: { name: 'CampaignTblAnalyticsDataTemplate' , foreach: $root.CampaignTblAnalyticsData } -->
                    <!-- /ko -->
                </table>
            </div>

        </div>
        <div class="clearfix">
        </div>
        <div class="clearfix">
        </div>
    </div>

</div>


<script id="CampaignTblAnalyticsDataTemplate" type="text/html">
    <tr>
        <td><span data-bind="text: label"></span></td>

        <td class="padingL15 text-right">
            <!-- ko if: ordr === 4  -->
            $
            <!-- /ko -->
            <span data-bind="text: All_time"></span>
        </td>
        <td class="padingL15 text-right">
            <!-- ko if: ordr === 4  -->
            $
            <!-- /ko -->
            <span data-bind="text: C30_days"></span>
        </td>
    </tr>
</script>


