    @{
        Html.RenderPartial("~/Areas/Ads/Views/Ads/ManageCampaign.cshtml");
    }
    
</div>
<script id="advertsTemplate" type="text/html">
    <tr style="cursor: pointer">
        <td class="col-sm-2">
            <a href="#" class="store-user-link" data-bind="text: CampaignName() ,click:$root.onEditCampaign"></a>
            <br />
            <!-- ko if: Type() == 4-->
            <img style="width:130px;height:130px;" data-bind=" attr:{src:ImagePath()!=null?ImagePath().startsWith('http')?ImagePath():'/' + ImagePath():null}">
            <!-- /ko -->
            <!-- ko if: Type() == 1-->
            <video style="width:130px;height:130px;" data-bind=" attr:{src:ImagePath()!=null?ImagePath().startsWith('http')?ImagePath():'/' + ImagePath():null}"></video>
            <!-- /ko -->
            <br />
            <!-- ko if: ApprovalDateTime()-->
            <span style="color:darkgray;font-size:11px;">
                Start Date :
            </span>
            <span style="color:darkgray;font-size:11px;" data-bind="dateString:  ApprovalDateTime()"></span>
            <!-- /ko -->
            <!-- ko if: ApprovalDateTime()== null -->
            <span style="color:darkgray;font-size:11px;">
                Not Started
            </span>

            <!-- /ko -->
            <!-- ko if: ModifiedDateTime()-->
            <br />
            <span style="color:darkgray;font-size:11px;">
                Last Updated :
            </span>

            <span style="color:darkgray;font-size:11px;" data-bind="dateString:  ModifiedDateTime()"></span>
            <!-- /ko -->
            @*<span style="color:darkgray;font-size:11px;" data-bind="text:  ApprovalDateTime() ? 'Start Date : ' +  ApprovalDateTime() : 'Not Started'"></span>
                <br />
                <span style="color:darkgray;font-size:11px;" data-bind="text:  ModifiedDateTime() ? 'Last Updated : ' +  ModifiedDateTime() : ''"></span>*@
        </td>
        <td class="col-sm-1"><span class="" data-bind="text: ClickRate()? '£'+ ClickRate(): '£0.0'"></span></td>
        <td class="col-sm-1"><span class="" data-bind="text: MaxDailyBudget()? '£'+ MaxDailyBudget(): '£0.0'"></span></td>
        <td class="col-sm-3" colspan="3" style="text-align: right;">
            <div class="col-sm-3"><span style="color:green" data-bind="text: viewCountAllTime()"></span></div>
            <div class="col-sm-3"><span style="color:blue" data-bind="text:SurveyAnsweredAllTime() "></span></div>
            <div class="col-sm-3"><span style="color:#ea2e94" data-bind="text: clickThroughsAllTime()"></span></div>
            <br />
            <div style="height:150px;width:215px;" data-bind="morris: {type: 'Bar', data: [{y:'test', a:viewCountAllTime() ,b:SurveyAnsweredAllTime() , c:clickThroughsAllTime()}],axes:false, xkey: 'y', ykeys: ['a' , 'b', 'c'], labels: ['Total Impression','Survey PPCV', 'Click Thru'], parseTime:false, setAxisAlignFirstX: true,hideHover:'always', barColors: ['green', 'blue','#ea2e94']}"></div>


        </td>
        <td class="col-sm-1">
            <div style="color:#ea2e94; font-size:larger;font-weight: bold;"><span data-bind="text:  viewCountAllTime()>0 ? ((clickThroughsAllTime()/viewCountAllTime())*100).toFixed(2):0"></span>%</div>

        </td>

        <td class="col-sm-1">
            <div style="color:blue; font-size:larger;font-weight: bold;"><span data-bind="text: viewCountAllTime()>0 ? ((SurveyAnsweredAllTime()/viewCountAllTime())*100).toFixed(2):0"></span>%</div>

        </td>

        <td class="col-sm-3">
            <!-- ko if: VerifyQuestion() != null -->
            <div style="font-size:small" data-bind="text:VerifyQuestion()"></div>
            <br />
            <div class="piechart">
                <canvas data-bind="chart: { type: 'pie', data: CampaignRatioData, options: { observeChanges: true, throttle: 1000, legend: { display: false }, maintainAspectRatio: false,responsive: true } }"></canvas>
            </div>
            <!-- /ko -->
        </td>

        <td class="col-sm-1">
            <table class="tbl-noborder-Padding">
                <tbody>
                    <tr>
                        <td>
                            <br />
                        </td>
                        <td><br /></td>
                    </tr>
                    <!-- ko if: Answer1() != null -->
                    <tr>
                        <td>
                            <span data-bind="text: Answer1"></span>
                        </td>
                        <td>
                            <span data-bind="text: SurveyAnsweredAllTime() > 0 ? ((Answer1Stats()/(SurveyAnsweredAllTime()))*100).toFixed(2): 0"></span>%
                        </td>
                    </tr>
                    <!-- /ko -->
                    <!-- ko if: Answer2() != null -->
                    <tr>
                        <td>
                            <span data-bind="text: Answer2"></span>
                        </td>
                        <td>
                            <span data-bind="text: SurveyAnsweredAllTime() > 0 ? ((Answer2Stats()/SurveyAnsweredAllTime())*100).toFixed(2):0"></span>%
                        </td>
                    </tr>
                    <!-- /ko -->
                    <!-- ko if: Answer3() != null -->
                    <tr>
                        <td>
                            <span data-bind="text: Answer3"></span>
                        </td>
                        <td>
                            <span data-bind="text: SurveyAnsweredAllTime() > 0 ? ((Answer3Stats()/SurveyAnsweredAllTime())*100).toFixed(2):0"></span>%
                        </td>
                    </tr>
                    <!-- /ko -->
                </tbody>
            </table>
        </td>

        <td class="col-sm-1">
            <div style="font-size:larger;font-weight:bold;">
                <!-- ko if: Status() == 1-->
                <span style="color:#6D6EFA; " data-bind="text: StatusValue">
                </span>
                <!-- /ko -->
                <!-- ko if: Status() == 2-->
                <span style="color:#fc0; " data-bind="text: StatusValue">
                </span>
                <!-- /ko -->
                <!-- ko if:  Status() == 3-->
                <span style="color:#37c675; " data-bind="text: StatusValue">
                </span>
                <!-- /ko -->
                <!-- ko if: Status() == 4-->
                <span style="color:#fd008e; " data-bind="text: StatusValue">
                </span>
                <!-- /ko -->
                <!-- ko if: Status() == 5-->
                <span style="color:#c8e392; " data-bind="text: StatusValue">
                </span>
                <!-- /ko -->
                <!-- ko if: Status() == 6-->
                <span style="color:red; " data-bind="text: StatusValue">
                </span>
                <!-- /ko -->
                <!-- ko if: Status() == 7-->
                <span style="color:red; " data-bind="text: 'Remove'">
                </span>
                <!-- /ko -->
                <!-- ko if: Status() == 8-->
                <span style="color:#fc0; " data-bind="text: StatusValue"> </span>
                <!-- /ko -->
                <!-- ko if: Status() == 9-->
                <span style="color:red; " data-bind="text: StatusValue"> </span>
                <!-- /ko -->
            </div>

            <br />
            <div class="text-left statusbg" data-bind="click:$root.onEditCampaign" style="background:#37c675;width:95px;white-space:nowrap;">
                <span>
                    Edit Campaign
                </span>

            </div>
            <br />
            <!-- ko if: Status() != 1 &&  Status() != 2 && Status() !=  6-->

            <div class="text-left statusbg" data-bind="click:$root.openAdvertiserDashboardScreen" style="background:blue;width:95px;white-space:nowrap;">
                <span>
                    Analyze Charts
                </span>
            </div>
            <br />
            <!-- /ko -->
            <div style="text-align: center;">
                <!-- ko if: Status() == 1 ||  Status() == 4|| Status() ==  6-->
                <a class="table-link danger">
                    <span class="fa-stack">
                        <i class="fa fa-square fa-stack-2x"></i>
                        <i class="fa fa-trash-o fa-stack-1x fa-inverse" data-bind="click:$root.removeAdd"></i>
                    </span>
                </a>
                <!-- /ko -->
            </div>

        </td>

    </tr>
</script>
@* Filter Template *@
<script id="filterTemplate" type="text/html">
    <div class="filter-body" style="padding-top:0p!important;" id="filter" data-bind="visible:!isDisplayCouponsAds()">
        <div class="row" id="topArea">
        </div>
        <div class="row" style="display:none;">
            <div class="col-sm-12" style="float: left;">

                <div style="width:100%;">
                    <span style="font-size:18px;float:left;color:#81d3dc!important;" data-bind="text:$root.MainHeading"></span>
                    <button style="float:right;margin-top:-15px;" class="btn btn-success create_Campaign_btn" data-bind="click:$root.addNewCampaign,visible:!$root.isDisplayCouponsAds()">
                        Create New Campaign
                    </button>
                    <div style="clear:both;"></div>
                </div>
                <div style="width:641px;word-wrap:break-word;">
                    <span style="font-size: 14px;" class="Theme_subheading-Clr " data-bind="text:$root.SubHeading"></span>  &nbsp; &nbsp; &nbsp; <span class="red bold rightAlign" style="display:none;"> Watch Video <img src="/Images/youtube_play.JPG" height="25px" /> </span>   </span><br />
                </div>
            </div>

        </div>

        <div class="row">

            <form data-bind="submit: $root.getCampaignByFilter">
                <div id="filter_block">

                    <div class="col-sm-2" style="float:left;">
                        <div class="filter-block " style="margin-right: -10px" data-bind="visible:$root.isAdSearch()==false">
                            <div class="form-group ">
                                <input type="text" class="form-control " placeholder="Search..." data-bind="value: $root.searchFilterValue">
                                <i class="fa fa-search search-icon" data-bind="click: $root.getCampaignByFilter"></i>
                            </div>
                        </div>
                        <div class="clearfix"></div>
                    </div>
                    <div class="col-sm-2 filterbloclkspos" style="float:left;">
                        <select id="ddlStatus" class="form-control" style="float:right;height:36px;" data-bind="value : SearchSelectedStatus, event:{ change: $root.getCampaignByFilter},visible:$root.isAdSearch()==false">
                            <option value="0">All Statuses</option>
                        </select>
                    </div>
                </div>
            </form>
            <div data-bind="visible:$root.islblText()==true">
                <div class="col-sm-4 filterbloclkspos" style="float:left;font-size: 20px;margin-top:6px;">
                    <span data-bind="visible:$root.IsGameAds()">Sponsor a brain training game !</span>
                    <span data-bind="visible:$root.IsvideoBtn()">Target Audiences with a Video Ad !</span>
                </div>

                <div class="col-sm-1">
                    <img src="/Images/Arrow.png" style="height:35px;">
                </div>
            </div>

            <div class="col-sm-3" style="float:right;margin-top:0px; text-align:right">
                @*<img class="defvideoimage" src="/Content/Images/Videos_small.png" data-bind="visible:$root.VideoImage">
                    <img class="Game_image" src="/Content/Images/Display_small.png" data-bind="visible:$root.DisplayImage">*@
                <div data-bind="visible:$root.IsvideoBtn()">
                    <button class=" btn btn-success create_Campaign_btn" style="" data-bind="click:$root.addNewCampaign,visible:!$root.isDisplayCouponsAds()">
                        Create Video Ad
                    </button>
                </div>
                <div data-bind="visible:$root.IsGameAds()">
                    <button class="btn btn-primary create_game_btn" style="background-color:#37c675!important;border-color:#37c675!important;" data-bind="click:$root.addNewCampaign,visible:!$root.isDisplayCouponsAds()">
                        Create Display Ad
                    </button>
                </div>
                @*<button class="btn btn-primary" style="display:none;" data-bind="click:$root.addNewCampaign,visible:$root.isDisplayCouponsAds()">
                        Create Coupon
                    </button>*@
            </div>
            <div class="clearfix">
            </div>
        </div>
    </div>
</script>
