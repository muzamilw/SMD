                                            <th class="col-sm-5" colspan="3">&nbsp;</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <!-- ko template: { name: 'advertsTemplate' , foreach: $root.campaignGridContent } -->
                                        <!-- /ko -->
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                    <div id="pagerDivForStore" class="pull-right">
                        <!-- ko with: pager -->
                        @Html.Partial("~/Areas/Common/Views/Shared/_Pager.cshtml")
                        <!-- /ko -->
                    </div>
                    <div class="clearfix">
                    </div>
                </div>
                <div data-bind="visible: ($root.campaignGridContent().length == 0 && $root.gridTotalCount() == 0)" style="display:none;">
                    <div class="clearfix text-center" style="padding:20px;padding-bottom:10px;" data-bind="visible:$root.IsGameAds()">
