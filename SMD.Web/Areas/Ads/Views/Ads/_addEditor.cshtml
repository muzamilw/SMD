@{
    ViewBag.Title = "Store Details";
}
<div data-bind="visible: $root.isEditorVisible()" style="display: none">
    <div class="col-lg-12">
        @*<div class="row">
                <div class="col-lg-12">
                    <ol class="breadcrumb">
                        <li><a href="#" class="my-navigator">Home</a></li>
                        <li><a href="#" class="my-navigator" >Ad Campaign Approval</a></li>
                    </ol>

                </div>
            </div>*@
        <div class="row" id="user-profile">
            <div class="col-lg-12 col-md-8 col-sm-8" style="padding-bottom: 10px;">
                <div class="clearfix ">
                    <div class="col-xs-12" style="text-align: right; padding-right:0;margin-bottom:1px;margin-top:10px;">

                        <button class="btn btn-default" id="saveBtn" data-bind="click: $root.closeEditDialog">Close</button>
                    </div>
                </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="main-box clearfix" id="tabsHeader">
                    <div class="tabs-wrapper profile-tabs">
                        <ul class="nav nav-tabs" style="display:none;">
                            <li class="active" id="generalInfoTab"><a href="#tab-generalinfo" data-toggle="tab">General Info</a></li>
                        </ul>
                        <div class="tab-content">
                            <div class="tab-pane fade in active" data-bind="with: $root.selectedCampaign" id="tab-generalinfo">
                                <div class="col-lg-12 form-horizontal">
                                    <h3 class="group-header" style="margin-top:20px;" data-bind="visible:type()==1">
                                   Approve - Reject Vidio Ads
                                    </h3>
                                    <h3 class="group-header" style="margin-top:20px;" data-bind="visible:type()==4">
                                        Approve - Reject Display Ads
                                    </h3>
                                    <div class="col-lg-9">
                                        <div class="form-group">
                                            <div class="col-lg-4 text-right">
                                                <label class="control-label text-right tooltip-float" data-bind="visible:!$root.isShowCopounMode()">Ad Title</label>
                                                <label class="control-label text-right tooltip-float" data-bind="visible:$root.isShowCopounMode()">Coupon Name</label>
                                            </div>
                                            <div class="col-lg-6">
                                                <input type="text" readonly class="form-control" data-bind="value:campaignName">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-9" style="display:none">
                                        <div class="form-group">
                                            <div class="col-lg-4 text-right">
                                                <label class="control-label text-right tooltip-float">Mode</label>
                                            </div>
                                            <div class="col-lg-6">
                                                <input type="text" readonly class="form-control" data-bind="value:deliveryDays">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-9" data-bind="visible:false">
                                        <div class="form-group">
                                            <div class="col-lg-4 text-right">
                                                <label class="control-label text-right tooltip-float">First line</label>
                                            </div>
                                            <div class="col-lg-6">
                                                <input type="text" readonly class="form-control" data-bind="value: description">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-9" data-bind="visible:false">
                                        <div class="form-group">
                                            <div class="col-lg-4 text-right">
                                                <label class="control-label text-right tooltip-float">Second line</label>
                                            </div>
                                            <div class="col-lg-6">
                                                <input type="text" readonly class="form-control" data-bind="value: CampaignDescription">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-9" data-bind="visible:$root.isShowCopounMode()">
                                        <div class="form-group">
                                            <div class="col-lg-6">
                                                <input type="text" readonly class="form-control" data-bind="value: CouponSwapValue">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-9" data-bind="visible:$root.isShowCopounMode()">
                                        <div class="form-group">
                                            <div class="col-lg-4 text-right">
                                                <label class="control-label text-right tooltip-float">Actual Value</label>
                                            </div>
                                            <div class="col-lg-6">
                                                <input type="text" readonly class="form-control" data-bind="value: CouponActualValue">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-9" data-bind="visible:$root.isShowCopounMode()">
                                        <div class="form-group">
                                            <div class="col-lg-4 text-right">
                                                <label class="control-label text-right tooltip-float">Quantity</label>
                                            </div>
                                            <div class="col-lg-6">
                                                <input type="text" readonly class="form-control" data-bind="value: CouponQuantity">
                                            </div>
                                        </div>
                                    </div>
