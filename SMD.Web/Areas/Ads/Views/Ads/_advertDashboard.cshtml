                            <td></td>
                        </tr>
                        <tr>
                            <td class="col-lg-3">Age range : </td>
                            <td class="col-lg-3"><select class="form-control" id="ddQQADropDown" data-bind="options: $root.AgeRangeAnalyticsData, optionsText: 'name', optionsValue: 'id', value:$root.selectedQQAAnalytics, event: { change: $root.getQQAnalytic }"></select></td>
                            <td class="col-lg-6"></td>
                        </tr>
                        <tr>
                            <td class="col-lg-3">City : </td>
                            <td class="col-lg-3"><select class="form-control" id="ddQQCtDropDown" data-bind="options: $root.CitiesAnalyticsData, optionsText: 'name', optionsValue: 'name', value:$root.selectedQQCtAnalytics, event: { change: $root.getQQAnalytic }"></select></td>
                            <td class="col-lg-6"></td>
                        </tr>

                    </table>
                </div>
            
            <div class="col-lg-12" data-bind="visible: $root.isProfileQuestionUsed()" >
                <br />
                <br />
                Profile Question
            </div>
           <div class="col-lg-12">
                <table class="targetfiltertbl">
                    <!-- ko template: { name: 'ProfileQuestionDataTemplate' , foreach: $root.formAnalyticsData } -->
                    <!-- /ko -->
                    </table>
               </div>
            <div class="col-lg-12" data-bind="visible: $root.isPreviousQuizQuestionsUsed()">
                <br />
                <br />
                Previous Quiz Questions used in campaign
            </div>
    <div class="col-lg-12">
        <table class="targetfiltertbl">
            <!-- ko template: { name: 'PreviousQuizQuestionDataTemplate' , foreach: $root.formAnalyticsData } -->
            <!-- /ko -->
        </table>
    </div>
            <div class="col-lg-12" data-bind="visible: $root.isPollQuestionsQuestionUsed()">
                <br />
                <br />
                Poll Questions
            </div>
    <div class="col-lg-12">
        <table class="targetfiltertbl">
            <!-- ko template: { name: 'SurvayQuestionDataTemplate' , foreach: $root.formAnalyticsData } -->
            <!-- /ko -->
        </table>
    </div>
            </div>
        <div class="clearfix">
        </div>
        <div class="clearfix">
        </div>
    </div>

</div>
