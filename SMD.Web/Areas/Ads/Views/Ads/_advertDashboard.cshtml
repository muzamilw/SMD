                            <td class="col-lg-3">
                                <span data-bind="text: $root.QuizQuestionAnalyticData"></span>

                            </td>
                            <td class="col-lg-3"><select class="form-control" id="ddQQCDropDown" data-bind="options: $root.QQChoicesAnalyticsData, optionsText: 'ChoiceDescription', optionsValue: 'ChoiceId', value:$root.selectedQQCAnalytics, event: { change: $root.getQQAnalytic }"></select></td>
                            <td class="col-lg-6">
                                = <span data-bind="text: $root.QQStatsAnalytics"></span>
                            </td>
                        </tr>
                        <tr>
                            <td class="col-lg-3">Gender : </td>
