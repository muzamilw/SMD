         <div data-bind="visible: $root.isAdvertdashboardVisible()" style="display: none" class=" col-lg-9 form-horizontal" style="padding-left:0px;">
            <div class="main-box main-box-subpage">
                <div class="row">
                    <div class="col-lg-12">
                        <br />
                        <h1 style="padding-left:7px!important;">Ad campaign timeline</h1>
                    </div>
                </div>
                <div >
                    <div class="col-lg-4"></div>
                    <div class="col-lg-8">
                        <select class="form-control margin10 pull-left" style="width:28%!important;" id="ddGranularityDropDown" data-bind="options: $root.granularityDropDown, optionsText: 'name', optionsValue: 'id', value:$root.selectedGranularityAnalytics, event: { change: $root.getAdsByCampaignIdAnalytics }"></select>
                        <select class="form-control margin10 pull-left" style="width:28%!important;" id="ddDateRangeDropDown" data-bind="options: $root.DateRangeDropDown, optionsText: 'name', optionsValue: 'id', value:$root.selecteddateRangeAnalytics, event: { change: $root.getAdsByCampaignIdAnalytics }"></select>
                        <select class="form-control margin10 pull-left" style="width:28%!important;" id="ddCampaignStatusDropDown" data-bind="options: $root.CampaignStatusDropDown, optionsText: 'name', optionsValue: 'id', value:$root.selectedCampStatusAnalytics, event: { change: $root.getAdsByCampaignIdAnalytics }"></select>
                    </div>

                </div>
                <div class="row main-box">
                    <div class="col-lg-12">
                        <div class=" margin10">
                            <!-- ko if: $root.selectedCampStatusAnalytics() === 1  -->
                            <div class="margin-Top-30" data-bind="morris: {type: 'Line', data: $root.AdsCampaignAnalyticsData, xkey: 'Granual', ykeys: ['openStats', 'avgAdClickStats', 'Stats'], labels: ['Total Impressions', 'Ad Click ($)','Answered'], parseTime:false, setAxisAlignFirstX: true, lineColors: ['blue','red','green'] }"></div>
                            <!-- /ko -->
                            <!-- ko if: $root.selectedCampStatusAnalytics() === 2  -->
                            <div class="margin-Top-30" data-bind="morris: {type: 'Line', data: $root.AdsCampaignAnalyticsData, xkey: 'Granual', ykeys: ['openStats', 'avgAdClickStats', 'Stats'], labels: ['Total Impressions', 'Ad Click ($)','Reffered'], parseTime:false, setAxisAlignFirstX: true, lineColors: ['blue','red','green'] }"></div>
                            <!-- /ko -->
                            <!-- ko if: $root.selectedCampStatusAnalytics() === 3  -->
                            <div class="margin-Top-30" data-bind="morris: {type: 'Line', data: $root.AdsCampaignAnalyticsData, xkey: 'Granual', ykeys: ['openStats', 'avgAdClickStats', 'Stats'], labels: ['Total Impressions', 'Ad Click ($)','Skipped'], parseTime:false, setAxisAlignFirstX: true, lineColors: ['blue','red','green'] }"></div>
                            <!-- /ko -->


                        </div>
                    </div>

                </div>
                <div class="clearfix">
                </div>
                <div class="clearfix">
                </div>
            </div>

        </div>


