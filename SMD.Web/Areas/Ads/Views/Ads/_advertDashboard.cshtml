

<div data-bind="visible: $root.isAdvertdashboardVisible()" style="display: none;padding-left:0px;" class=" col-lg-12 form-horizontal">
    <div class="main-box main-box-subpage">
        <div class="row">

            <span class="charts_label" style="margin-left: 16px;">Charts & Analytics</span>

            <div id="roor_panel" style="margin-top:11px;">
                <div class="col-lg-6 adsubHeading Theme_standard_clr borderbtmline" style="padding-bottom:10px;padding-left:15px;">
                    Timeline
                </div>
                <div class="col-lg-3 adsubHeading Theme_standard_clr borderbtmline" style="padding-bottom:10px;">
                    <label class="control-label text-right tooltip-float floatNone defaultThemeTxtClr defaultImgUploadImageBtn">Date range </label>
                </div>
                <div class="col-lg-3 adsubHeading Theme_standard_clr borderbtmline" style="padding-bottom:2px;">
                    <select class="form-control" id="ddDateRangeDropDown" data-bind="options: $root.DateRangeDropDown, optionsText: 'name', optionsValue: 'id', value:$root.selecteddateRangeAnalytics, event: { change: $root.getAdsByCampaignIdAnalytics }"></select>
                </div>
            </div>
        </div>
        <div id="ddpanel">
            <div class="col-lg-3"></div>
            <div class="col-lg-1">
              
            </div>
            <div class="col-lg-3">
                <select class="form-control margin10 pull-left" id="ddCampaignStatusDropDown" data-bind="options: $root.CampaignStatusDropDown, optionsText: 'name', optionsValue: 'id', value:$root.selectedCampStatusAnalytics, event: { change: $root.getAdsByCampaignIdAnalytics }"></select>
            </div>
            <div class="col-lg-2">
                <label style="padding-top:18px;" class="control-label text-right tooltip-float floatNone defaultThemeTxtClr defaultImgUploadImageBtn">Granularity level </label>
            </div>
            <div class="col-lg-3">
                <select class="form-control margin10 pull-left" id="ddGranularityDropDown" data-bind="options: $root.granularityDropDown, optionsText: 'name', optionsValue: 'id', value:$root.selectedGranularityAnalytics, event: { change: $root.getAdsByCampaignIdAnalytics }"></select>
            </div>

            

        </div>
        <div class="row main-box">
            <div class="col-lg-12">
                <div class=" margin10">
                    <!-- ko if: $root.selectedCampStatusAnalytics() === 1  -->
                    <div style="height:150px;" data-bind="morris: {type: 'Line', data: $root.AdsCampaignAnalyticsData, xkey: 'Granual', ykeys: ['openStats', 'Stats'], labels: ['Total Impressions', 'Answered'], yLabelFormat: function(y){return y != Math.round(y)?'':y;}, parseTime:false, setAxisAlignFirstX: true, lineColors: ['blue','green'] }"></div>
                    <!-- /ko -->
                    <!-- ko if: $root.selectedCampStatusAnalytics() === 2  -->
                    <div style="height:150px;" data-bind="morris: {type: 'Line', data: $root.AdsCampaignAnalyticsData, xkey: 'Granual', ykeys: ['openStats', 'Stats'], labels: ['Total Impressions', 'Reffered'], yLabelFormat: function(y){return y != Math.round(y)?'':y;},  parseTime:false, setAxisAlignFirstX: true, lineColors: ['blue','green'] }"></div>
                    <!-- /ko -->
                    <!-- ko if: $root.selectedCampStatusAnalytics() === 3  -->
                    <div style="height:150px;" data-bind="morris: {type: 'Line', data: $root.AdsCampaignAnalyticsData, xkey: 'Granual', ykeys: ['openStats', 'Stats'], labels: ['Total Impressions', 'Skipped'], yLabelFormat: function(y){return y != Math.round(y)?'':y;},  parseTime:false, setAxisAlignFirstX: true, lineColors: ['blue','red'] }"></div>
                    <!-- /ko -->


                </div>
            </div>

        </div>

        @*<div class="row main-box">
            <div class="col-lg-12">
