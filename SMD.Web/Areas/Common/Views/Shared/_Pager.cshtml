<div data-bind="if: pagerWithLimit().length > 0" style="text-align:right;margin-top:0px;">
    <ul class="pagination">
        <li><a href="#" data-bind="click: previousPage"><i class="fa fa-chevron-left"></i></a></li>
        <!-- ko if: showBackPageDotLink -->
        <li><a href="#" data-bind="click: shiftRight">...</a></li>
        <!-- /ko -->
        <!-- ko foreach: pagerWithLimit -->
        <li data-bind="css: { active: page === $parent.currentPage() }">
            <a href="#" data-bind="text: page, click: clicked">1</a>
        </li>
        <!-- /ko -->
        <!-- ko if: showNextPageDotLink -->
        <li><a href="#" data-bind="click: shiftLeft">...</a></li>
        <!-- /ko -->
        <li><a href="#" data-bind="click: nextPage"><i class="fa fa-chevron-right"></i></a></li>
    </ul>
</div>
<div class="searchCount" style="text-align:right;">
    <span data-bind="text: filteredTotalContents"></span>
</div>

