@{
    var userRptSrc = "";
    var publisherRptSrc = "";
    if (Session["User"] != null) 
    {
        Cash4Ads.Models.User sessionVar = Session["User"] as Cash4Ads.Models.User;
        
            DateTime todayDate = DateTime.Today;

            DateTime LastMonthStartDT = todayDate.AddMonths(-1);
            userRptSrc = "http://manage.cash4ads.com/reportViewers/user.aspx?userID=" + sessionVar.UserId + "&StartDate=" + DateTime.Now.AddMonths(-1).ToString("M-d-yyyy") + "&EndDate=" + DateTime.Now.ToString("M-d-yyyy"); //11-01-2015 //12-26-2016
        
            publisherRptSrc = "http://manage.cash4ads.com/reportViewers/publisher.aspx?CompanyId=" + sessionVar.CompanyId;
       
    }
 
}
<div id="reportpopin" class="popUpOuterLayer" style="display:none;">
    <div class="innerLayer">
       
        <h3 class="appollo_rptViewrheading">
            <span id="reportHeadin">My Activity Log</span><i class="fa fa-times close_popp" aria-hidden="true" onclick="closeRptViewer();"></i>
        </h3>
        <div class="apollo_rptViewer">
            <iframe src="" id="frameRptViewr" width="100%" height="500px"></iframe>
        </div>
    </div>

</div>

<script>
    function openRptViewer(rptType) {
        var bws = getBrowserHeight();
        var top = parseInt((bws.height - 550) / 2);
        if (rptType == 1)
        {
            $("#frameRptViewr").attr("src", "@userRptSrc");
            $("#reportHeadin").text("My Account Activity");
        }
        else if (rptType == 2)
        {
            $("#frameRptViewr").attr("src", "@publisherRptSrc");
            $("#reportHeadin").text("My Campaign Activity");
        }
        $("#reportpopin").css("display", "block");
        $(".appollo_rptViewrheading").css("top", top + "px");
        $(".apollo_rptViewer").css("top", top + "px");
    }
    function closeRptViewer() {
        $("#reportpopin").css("display", "none");
    }
</script>