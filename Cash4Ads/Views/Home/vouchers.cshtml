    $('#selcat').on('change', function() {
        $("#bonus-deals-side").css('display','block');
        getCoupons();
    });

    function LoadSelectedVoucher(CouponId) {

        var couponObj = $.grep(Coupons, function (n, i) {
            return (n.CouponId == CouponId);
        });

        
        if (couponObj != null) {
            $("#inner_coupon_cont,#main_copon_cont").css('display','block');
            $("#bonus-deals-side,.extraMargingStandardContainer").css('display','none');
            //$("#couponMainHeading").html(couponObj[0].CouponTitle + '<span class="mvtest_header_v1">' + couponObj[0].Firstline + ' ' + couponObj[0].SecondLine + '</span><div class="btm-header-bg"></div>');

            $("#couponMainHeading").html(couponObj[0].CouponTitle + '<span class="mvtest_header_v1">' + '' + '</span><div class="btm-header-bg"></div>');
            $("#voucMainSliderImg").attr('src', couponObj[0].CouponImage1);

            $("#couDiscountedVal").html(couponObj[0].Savings);
            $("#couActualVal").html(couponObj[0].Price);

            
            $("#coupPerct").text(100 - parseInt((parseFloat(couponObj[0].Savings) / parseFloat(couponObj[0].Price)) * 100) + '%');
           // $("#coupBought").text(couponObj[0].CouponTakenValue);
            $("#mainDealBuy").attr('data-couponId', couponObj[0].CouponId);
        }

    }
    function BuyCoupon(){
        if("@sessionVar" == ""){
            alert("Please login to buy voucher.");

        }else{
            var urlToSite = "";
            urlToSite = 'http://smdpreview.com/Api/RedeemCoupon?authenticationToken='+ '@ViewBag.userid' +'&companyId='+ '@ViewBag.companyid' +'&CouponId='+ $("#mainDealBuy").attr('data-couponId') +'&mode=3&amount='+ $("#couDiscountedVal").text() +'';
           
           
            $.ajax({
                dataType: "json",
                url: urlToSite,
                cache: false,
                success: function (response) {
                    if(response == false){
                        alert("Can't purchase voucher because balance is not sufficient.");
                    }else{
                        alert("Voucher redeemed successfully.");
                    }
                }
            });
        }

        
    }
    function LoadCategories()
    {
            
        GetCategories();
        $("#inner_coupon_cont,#main_copon_cont,#toggle_btn").css('display','none');
        $("#bonus-deals-side,.extraMargingStandardContainer").css('display','block');

    }
    function Toggle()
    {
        LoadCategories();
    }

    function NavigateToDeal(id)
    {
        var url = "/Deal?id=" + id; 
        window.location.href = url; 
    }

    function NavigateOwn()
    {
        $("#bonus-deals-side").html('');
        getReqCoupons();
        $("#selcat").prop('selectedIndex', 0);
        $('#couponsNotFound').hide();
        $('#couponsContains').show();
    }
</script>